{
  "expo": {
    "name": "MealzNow",
    "slug": "mealznow",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./src/assets/MNPublishingLogo512.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/MNPublishingLogo512.png",
      "resizeMode": "contain",
      "backgroundColor": "#08242D"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "co.foodsnow.MealzNow",
      "runtimeVersion": {
        "policy": "sdkVersion"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We use your location to offer a personalized experience by showing you nearby restaurants and ensuring accurate delivery services.",
        "NSLocationAlwaysUsageDescription": "Our app needs to access your location even when not in use to provide ongoing delivery updates and notify you when your order is nearby."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/MNPublishingLogo512.png",
        "backgroundColor": "#ffffff"
      },
      "runtimeVersion": {
        "policy": "sdkVersion"
      },
      "package": "co.foodsnow.MealzNow"
    },
    "web": {
      "favicon": "./src/assets/MNPublishingLogo512.png"
    },
    "extra": {
      "eas": {
        "projectId": "94e7f192-b914-4f1d-ba9c-a40eed5540aa"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/94e7f192-b914-4f1d-ba9c-a40eed5540aa"
    },
    "plugins": [
      "expo-build-properties"
    ],
    "owner": "foodsnowgmbh"
  }
}
